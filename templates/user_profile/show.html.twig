{% extends 'base.html.twig' %}

{% block title %}
    My Profile {{ user.email }}
{% endblock %}
{% block body %}
<div class="card">
    <div class="card-header">
        {{ user.email }}
    </div>
    <div class="card-body">
        <img src="{{ user.avatarUri }}" alt="avatar">
    </div>
</div>
{% endblock %}



